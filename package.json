{
  "name": "reactk-ts-sdk",
  "version": "0.0.1",
  "description": " Official Typescript / Javascript SDK for ReactK retention & user engagement analytics",
  "main": "./lib/reactk.js",
  "typings": "./lib/reactk.d.ts",
  "scripts": {
    "test": "jest test",
    "build": "npm run build:lib && npm run build:dist && npm run build:squash",
    "build:lib": "tsc",
    "build:dist": "browserify lib/reactk.js > dist/reactk.js",
    "build:squash": "squash dist/reactk.js > dist/reactk.min.js"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "user engagement analytics",
    "retention analytics"
  ],
  "author": "RÃ©mi Demol",
  "license": "Apache v2",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "babel-jest": "^23.4.2",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "browserify": "latest",
    "butternut": "latest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "(/src/.*\\.spec\\.ts)|(/test/.*\\.ts)$"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "moment": "^2.22.2",
    "uuid": "3.3.2"
  }
}
